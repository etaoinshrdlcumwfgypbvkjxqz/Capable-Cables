<?xml version="1.0" encoding="UTF-8" ?>
<theme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:val="https://github.com/etaoinshrdlcumwfgypbvkjxqz/Capable-Cables/schemas/ui/values"
       xmlns="https://github.com/etaoinshrdlcumwfgypbvkjxqz/Capable-Cables/schemas/ui/components"
       xml:lang="EN">
	<!-- SECTION using -->
	<!-- COMMENT renderers -->
	<using alias="renderer.component.window.debug.custom"
	       target="io.github.etaoinshrdlcumwfgypbvkjxqz.capablecables.ui.debug.UIMinecraftDebug$DebugUI$CustomWindowMinecraftComponentRenderer"/>
	<using alias="renderer.extension.minecraft.background.default"
	       target="io.github.etaoinshrdlcumwfgypbvkjxqz.capablecables.ui.impl.minecraft.mvvm.extensions.background.UIMinecraftBackgroundExtensionDefaultBackgroundRenderer"/>
	<using alias="renderer.extension.user_relocatable.previewing.minecraft"
	       target="io.github.etaoinshrdlcumwfgypbvkjxqz.capablecables.ui.impl.minecraft.mvvm.views.components.extensions.relocatable.UIComponentUserRelocatableMinecraftExtensionPreviewingRelocatingRenderer"/>
	<using alias="renderer.extension.user_resizable.previewing.minecraft"
	       target="io.github.etaoinshrdlcumwfgypbvkjxqz.capablecables.ui.impl.minecraft.mvvm.views.components.extensions.resizable.UIComponentUserResizableMinecraftExtensionPreviewingResizingRenderer"/>

	<!-- SECTION renderers -->
	<renderer name="ViewBackgroundRenderer" class="renderer.extension.minecraft.background.default"/>

	<renderer name="Window1Renderer" class="renderer.component.window.debug.custom">
		<property key="default:property.window.border.color">
			<val:color green="128"/>
		</property>
	</renderer>

	<renderer name="Window1ExtensionRelocatableRenderer"
	          class="renderer.extension.user_relocatable.previewing.minecraft">
		<property key="default:property.previewing.preview.color">
			<val:color green="128"/>
		</property>
	</renderer>

	<renderer name="Window1ExtensionResizableRenderer"
	          class="renderer.extension.user_resizable.previewing.minecraft"/>

	<renderer name="WindowAnchoredRenderer">
		<property key="default:property.window.border.color" bindingKey="default:anchoredWindowBorderColor"/>
	</renderer>

	<renderer name="WindowAnchoredExtensionRelocatableRenderer"
	          class="renderer.extension.user_relocatable.previewing.minecraft"/>

	<renderer name="WindowAnchoredExtensionResizableRenderer"
	          class="renderer.extension.user_resizable.previewing.minecraft"/>

	<renderer name="Button1Renderer">
		<property key="default:property.button.hovering.color">
			<val:color xsi:nil="true"/>
		</property>
	</renderer>
</theme>
