<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:val="https://github.com/etaoinshrdlcumwfgypbvkjxqz/Capable-Cables/schemas/ui/components/values"
           targetNamespace="https://github.com/etaoinshrdlcumwfgypbvkjxqz/Capable-Cables/schemas/ui/components/values"
           version="1.3.0"
           xml:lang="EN">
	<!-- SECTION primitives -->

	<!-- COMMENT boolean -->
	<xs:element name="boolean" type="xs:boolean" nillable="true"/>
	<!-- COMMENT byte -->
	<xs:element name="byte" type="xs:byte" nillable="true"/>
	<!-- COMMENT short -->
	<xs:element name="short" type="xs:short" nillable="true"/>
	<!-- COMMENT int -->
	<xs:element name="int" type="xs:int" nillable="true"/>
	<!-- COMMENT long -->
	<xs:element name="long" type="xs:long" nillable="true"/>
	<!-- COMMENT float -->
	<xs:element name="float" type="xs:float" nillable="true"/>
	<!-- COMMENT double -->
	<xs:element name="double" type="xs:double" nillable="true"/>

	<!-- SECTION objects -->

	<!-- COMMENT String -->
	<xs:element name="string" type="xs:string" nillable="true"/>

	<!-- COMMENT Color -->
	<xs:element name="color" nillable="true">
		<xs:complexType>
			<xs:attribute name="red" type="xs:unsignedByte" default="00"/>
			<xs:attribute name="green" type="xs:unsignedByte" default="00"/>
			<xs:attribute name="blue" type="xs:unsignedByte" default="00"/>
			<xs:attribute name="alpha" type="xs:unsignedByte" default="255"/>
		</xs:complexType>
	</xs:element>

	<!-- SECTION collections -->

	<!-- COMMENT Set -->
	<xs:element name="set" type="val:collectionType" nillable="true"/>

	<!-- COMMENT List -->
	<xs:element name="list" type="val:collectionType" nillable="true"/>

	<!-- COMMENT relations (set multi-map) -->
	<xs:element name="relations-set" type="val:relationsType" nillable="true"/>

	<!-- COMMENT relations (list multi-map) -->
	<xs:element name="relations-list" type="val:relationsType" nillable="true"/>

	<!-- COMMENT map -->
	<xs:element name="map" type="val:relationsType" nillable="true"/>

	<!-- SECTION types -->

	<!-- COMMENT collection type -->
	<xs:complexType name="collectionType">
		<xs:sequence>
			<xs:any minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<!-- COMMENT relations type -->
	<xs:complexType name="relationsType">
		<xs:sequence>
			<xs:element name="entry" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="key">
							<xs:complexType>
								<xs:sequence>
									<xs:any/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="value">
							<xs:complexType>
								<xs:sequence>
									<xs:any/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
